"language": "GDScript",
"path": "res://scripts/HUD.gd"
}, {
"base": "Area2D",
"class": "Movable",
"language": "GDScript",
"path": "res://scripts/Movable.gd"
}, {
"base": "Movable",
"class": "Player",
"language": "GDScript",
"path": "res://scripts/Player.gd"
}, {
